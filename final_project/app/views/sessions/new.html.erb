    <div class="list_head">
      <h2>Listing Product</h2>
    </div>
    <div class="content">
      <ul>
        <% @products.each do  |product| %>
        <li class="list_product">
          <div>
            <p>Name: </p><p class="product"><%=product.name%></p>
            <br/>
            <p>Price: </p><p class="product"><%=product.price%></p>
            <br/>
            <%= link_to "See Details",product_path(product.id),:method => :get, :title => "See Details"%>
                <% if current_user.present? && product.user.present? && current_user.id==product.user.id %>
                 | <%= link_to "Edit",edit_product_path(product.id),:method => :get, :title => "Edit Product"%>
                  | <%= link_to "Delete", product_path(product.id),:method => :delete, :title => "Delete Product"%>
                  <% end %>
          </div>
        </li>
        <% end %>
      </ul>
    </div>
    <br/ ><br />
  <div class="list_head">
      <h2>Listing Article</h2>
    </div>
    <div class="content" style="padding: 50px 0 0 0 ;">
      <table>
            <% i = 1 %>
        <% @articles.each do  |article| %>
            <% if i%2==0 %>
          <tr class="genap">
          <% else %>
          <tr class="ganjil">
          <% end %>
          <% i+=1 %>
            <td>
              <%= truncate(article.body, :length => 20, :omission => '...') %>
              <br/>
              <br/>
              <%= link_to "See Details",article_path(article.id),:method => :get, :title => "See Details"%>
                <% if current_user.present? && article.user.present? && current_user.id==article.user.id %>
                 | <%= link_to "Edit",edit_article_path(article.id),:method => :get, :title => "Edit article"%>
                  | <%= link_to "Delete", article_path(article.id),:method => :delete, :title => "Delete article"%>
                  <% end %>
            </td>
          </tr>
        <% end %>
      </table>
    </div>