<div class="wrap">
  <p><%=flash[:error]%></p>
  <p><%=flash[:notice]%></p>
  <h1>Log in</h1> 
    <%= form_tag sessions_path do %> 
    <p> 
      <%= label_tag :email %><br /> 
      <%= text_field_tag :email, params[:email] %> 
    </p> 
    <p> 
      <%= label_tag :password %><br /> 
      <%= password_field_tag :password %> 
    </p> 
    <%#= recaptcha_tags %>
    <%= link_to "Sign Up", new_user_path %>
    <p class="button"><%= submit_tag "Login" %></p> 
  <% end %> 
</div>